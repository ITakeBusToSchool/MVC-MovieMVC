@model List<Clone_MovieMVC_Entities.Movie>
@{
    ViewBag.Title = "TopMovies";
}

<table class="table">
    <thead>
        <tr>
            <td>
                Id
            </td>
            <td>
                Title
            </td>
            <td>
                Revenue
            </td>
            <td>
                Release Date
            </td>

        </tr>
    </thead>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Id</td>
            <td>@movie.Title</td>
            <td>@movie.Revenue</td>
            <td>@movie.ReleaseDate</td>
        </tr>
    }

</table>

